{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "Packages": {
            "matchCondition": {
                "route": "/packages/{id}",
                "methods": ["GET"]
            },
            "backendUri": "https://%WEBSITE_HOSTNAME%/api/Atom",
            "requestOverrides": {
                "backend.request.method": "get",
                "backend.request.querystring.id": "{id}"
            }
        }
    }
}