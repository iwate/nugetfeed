{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "Packages": {
            "matchCondition": {
                "route": "/packages/{*id}",
                "methods": [
                    "GET"
                ]
            },
            "backendUri": "https://%WEBSITE_HOSTNAME%/api/Atom",
            "requestOverrides": {
                "backend.request.querystring.id": "{id}"
            }
        }
    }
}